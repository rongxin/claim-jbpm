      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >Boolean managerApproval = (Boolean)kcontext.getVariable("managerApproval");
return managerApproval;</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_jbpm-unique-0-_jbpm-unique-2" sourceRef="_jbpm-unique-0" targetRef="_jbpm-unique-2" />
    <sequenceFlow id="ExclusiveGateway_1-_jbpm-unique-5" sourceRef="ExclusiveGateway_1" targetRef="_jbpm-unique-5" name="Not approved" tns:priority="1" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >Boolean managerApproval = (Boolean)kcontext.getVariable("managerApproval");
return true;</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_jbpm-unique-2-ExclusiveGateway_1" sourceRef="_jbpm-unique-2" targetRef="ExclusiveGateway_1" />

